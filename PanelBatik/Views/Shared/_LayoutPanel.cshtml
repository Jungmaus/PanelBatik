@{
    if (Session["Email"] == null)
    {
        Response.Redirect(Url.Action("Login", "Panel"));
    }
}

<!DOCTYPE html>
<html lang="en" ng-app="PanelApp">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>BuSepetten - Panel</title>

    <!-- Bootstrap Core CSS -->
    <link href="~/DesignFiles/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="~/DesignFiles/vendor/metisMenu/metisMenu.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="~/DesignFiles/dist/css/sb-admin-2.css" rel="stylesheet">

    <!-- Morris Charts CSS -->
    <link href="~/DesignFiles/vendor/morrisjs/morris.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="~/DesignFiles/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>

</head>

<body>

    <div id="wrapper">

        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("GostergePaneli","Panel")">BUSEPETTEN PANEL</a>
            </div>
            <!-- /.navbar-header -->

            <ul class="nav navbar-top-links navbar-right">
                <!-- /.dropdown -->
                <!-- /.dropdown -->
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-user fa-fw"></i> <i class="fa fa-caret-down"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                        <li>
                            <a href="@Url.Action("Ayarlar","Panel")"><i class="fa fa-gear fa-fw"></i> Ayarlar</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="@Url.Action("Logout","Operation")"><i class="fa fa-sign-out fa-fw"></i> Çıkış Yap</a>
                        </li>
                    </ul>
                    <!-- /.dropdown-user -->
                </li>
                <!-- /.dropdown -->
            </ul>
            <!-- /.navbar-top-links -->

            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li class="sidebar-search">
                            <div class="input-group custom-search-form">
                                <input type="text" class="form-control" placeholder="Search...">
                                <span class="input-group-btn">
                                    <button class="btn btn-default" type="button">
                                        <i class="fa fa-search"></i>
                                    </button>
                                </span>
                            </div>
                            <!-- /input-group -->
                        </li>
                        <li>
                            <a href="@Url.Action("GostergePaneli","Panel")"><i class="fa fa-dashboard fa-fw"></i> Gösterge Paneli</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-bar-chart-o fa-fw"></i> Ürün Yönetimi<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="@Url.Action("UrunListesi","Panel")">Ürün Listesi <i class="glyphicon glyphicon-th-list"></i></a>
                                </li>
                                <li>
                                    <a href="@Url.Action("YeniUrun","Panel")">Yeni Ürün Ekle <i class="glyphicon glyphicon-plus"></i></a>
                                </li>
                                <li>
                                    <a href="@Url.Action("GtinSorgula","Panel")">GTIN Sorgulama <i class="glyphicon glyphicon-barcode"></i></a>
                                </li>
                                <li>
                                    <a href="@Url.Action("KategoriListesi","Panel")">Kategori Listesi <i class="glyphicon glyphicon-tag"></i></a>
                                </li>
                                <li>
                                    <a href="@Url.Action("KategoriEkle","Panel")">Kategori Ekle <i class="glyphicon glyphicon-plus"></i></a>
                                </li>
                                <li>
                                    <a href="@Url.Action("KategoriListesi","Panel")">Kategoriye Ait Ürünler <i class="glyphicon glyphicon-tags"></i></a>
                                </li>
                                <li>
                                    <a href="@Url.Action("UrunListesiExcell","Panel")"><i class="fa fa-link fa-fw"></i>Ürün Listesini EXCELL'e Aktar</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("TopluUrunEkleExcell","Panel")"><i class="fa fa-link fa-fw"></i>EXCELL'den Toplu Ürün Ekle</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-table fa-fw"></i> Sipariş Yönetimi<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="@Url.Action("SiparisListesi","Panel")">Sipariş Listesi <i class="glyphicon glyphicon-th-list"></i></a>
                                </li>
                                <li>
                                    <a href="@Url.Action("SiparisBul","Panel")">Sipariş Bul <i class="glyphicon glyphicon-search"></i></a>
                                </li>
                                <li>
                                    <a href="@Url.Action("SiparisListesiExcell","Panel")"><i class="fa fa-link fa-fw"></i>Siparişleri EXCELL'e Aktar</a>
                                </li>
                            </ul>
                        </li>
                        <!-- /.nav-second-level -->
                    </ul>
                </div>
                <!-- /.sidebar-collapse -->
            </div>
            <!-- /.navbar-static-side -->
        </nav>

        <div id="page-wrapper">
            <div class="row">


                @RenderBody()


            </div>
        </div>
        <!-- /.panel-body -->
    </div>


    <!-- /#wrapper -->
<!-- jQuery -->

    <script src="~/DesignFiles/JavascriptFiles/PanelNG.js"></script>


    <!-- Bootstrap Core JavaScript -->
    <script src="~/DesignFiles/vendor/bootstrap/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/DesignFiles/vendor/metisMenu/metisMenu.min.js"></script>

    <!-- Morris Charts JavaScript -->
    <script src="~/DesignFiles/vendor/raphael/raphael.min.js"></script>
    <script src="~/DesignFiles/vendor/morrisjs/morris.min.js"></script>
    <script src="~/DesignFiles/data/morris-data.js"></script>

    <!-- Custom Theme JavaScript -->
<script src="~/DesignFiles/dist/js/sb-admin-2.js"></script>

</body>

</html>
